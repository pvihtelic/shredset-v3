<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "shredset" %></title>
    <meta name="shredset" content="">
    <meta name="shredset" content="">
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application", :media => "all" %>
    <%= javascript_include_tag "chosen.jquery" %>
    <%= javascript_include_tag "jquery.min" %>
    <%= csrf_meta_tags %>
    <link href='http://fonts.googleapis.com/css?family=Sanchez' rel='stylesheet' type='text/css'>
    <%= yield(:head) %>
  </head>
  <body>
    <header class="navbar navbar-fixed-top">
      <nav class="navbar-inner">
        <div class="container-fluid">
         <a class="brand" href="/"></a>
          <%= render 'layouts/navigation' %>
      </div>     
      </nav>
    </header>
    <div id="wrap">
    <div id="main" role="main">
      <div class="container">
           
              <%= yield %>
            
      </div> <!--! end of .container -->
    </div> <!--! end of #main -->
  </div>
  <footer class="footer">
            <ul class="footer-nav">
              <li class="footer-nav"><%= link_to "About Us", root_url %></li> |
              <li class="footer-nav"><%= link_to "FAQ", root_url %></li> |
              <li class="footer-nav"><%= link_to "Blog", root_url %></li> |
              <li class="footer-nav"><%= link_to "Share", root_url %></li> |
              <li class="footer-nav"><%= link_to "Contact", root_url %></li> |
              <li class="footer-nav"><%= link_to "Privacy", root_url %></li> |
              <li class="footer-nav"><%= link_to "Terms of Service", root_url %></li> 
            </ul>
          </footer>
  </body>
</html>
